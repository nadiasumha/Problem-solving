#include<iostream>
using namespace std;
int main()
{
    int n , ca=0 , cd=0;
    cin>>n;
    char a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        if(a[i]=='A')
            ca++;
        else if(a[i]=='D')
            cd++;}

     if(ca>cd)
        cout<<"Anton";
     else if(ca<cd)
        cout<<"Danik";
     else if(ca==cd)
        cout<<"Friendship";
}
